{
  "name": "@indocal/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "prisma generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "build": "nest build",
    "start": "nest start",
    "start:prod": "node dist/main"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/prisma/seed.ts"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "2.0.3",
    "@anatine/zod-openapi": "2.2.1",
    "@indocal/auth": "workspace:*",
    "@indocal/schemas": "workspace:*",
    "@nestjs/common": "10.2.10",
    "@nestjs/core": "10.2.10",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.2.10",
    "@nestjs/swagger": "7.1.16",
    "@prisma/client": "5.7.0",
    "bcrypt": "5.1.1",
    "nestjs-prisma": "0.22.0",
    "openapi3-ts": "4.1.2",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@indocal/eslint-config-custom": "workspace:*",
    "@indocal/tsconfig": "workspace:*",
    "@nestjs/cli": "10.2.1",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.2.10",
    "@types/bcrypt": "5.0.2",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@types/passport-jwt": "3.0.13",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "29.7.0",
    "prettier": "3.1.0",
    "prisma": "5.7.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
